@model DynaDevFE.Models.NhaCungCapViewModel

<form method="post">
    <div class="form-group">
        <label for="TenNCC">Tên Nhà Cung Cấp</label>
        <input type="text" id="TenNCC" name="TenNCC" class="form-control" />
    </div>

    <div class="form-group">
        <label for="SDT">Số Điện Thoại</label>
        <input type="text" id="SDT" name="SDT" class="form-control" />
    </div>

    <div class="form-group">
        <label for="Email">Email</label>
        <input type="email" id="Email" name="Email" class="form-control" />
    </div>

    <div class="form-group">
        <label for="DiaChi">Địa Chỉ</label>
        <input type="text" id="DiaChi" name="DiaChi" class="form-control" />
    </div>

    <div class="form-group">
        <label for="TinhTrang">Tình Trạng</label>
        <input type="text" id="TinhTrang" name="TinhTrang" class="form-control" />
    </div>

    <button type="submit" class="btn btn-primary">Thêm Nhà Cung Cấp</button>
</form>
